{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react",
      "allowJs": true,
      "checkJs": false,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "baseUrl": "./src"
    },
    "include": ["src"],
    "exclude": ["node_modules"]
  }
  